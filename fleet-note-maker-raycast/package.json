{
  "name": "fleet-note-maker",
  "title": "Fleet Note Maker",
  "description": "빠르게 메모를 작성하고 Fleet 폴더에 저장하는 생산성 도구",
  "icon": "command-icon.png",
  "author": "your-raycast-username",
  "license": "MIT",
  "version": "1.0.0",
  "commands": [
    {
      "name": "create-note",
      "title": "Fleet Note Maker",
      "description": "메모를 작성하고 Fleet 폴더에 저장합니다",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "savePath",
      "type": "textfield",
      "required": true,
      "title": "저장 경로",
      "description": "Fleet Note를 저장할 폴더의 절대 경로",
      "default": "~/agents/memo2fleet",
      "placeholder": "/Users/yourname/agents/memo2fleet"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.87.0",
    "@raycast/utils": "^1.19.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "^20.17.10",
    "@types/react": "18.2.79",
    "eslint": "^8.57.1",
    "prettier": "^3.4.2",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  },
  "overrides": {
    "@types/react": "18.2.79"
  }
}
